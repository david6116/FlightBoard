<!--a !logged in user will see this-->
<!doctype html>
<html lang="en">
<head>
    <%- include ('../partials/head.ejs') %>
    <title>Flight Board</title>
</head>
<body>
<%- include ('../partials/header.ejs') %>
    <div class="table-container">
        <table class="table text-center">
            <tr>
                <th>Flight #</th>
                <th>Est Arrival</th>
                <th>Origin</th>
                <th>Dest</th>
            </tr>
            <% for (const item in data){ %>
            <tr>
                <th><a href='/show/<%=data[item].ident %>'><%=data[item].ident %></a></th>
                <th><%=data[item].estimatedarrivaltime %></th>
                <th><%=data[item].origin %></th>
                <th><%=data[item].destination %></th>
            </tr>
            <% }%>
        </table>
    </div>
</body>
</html>